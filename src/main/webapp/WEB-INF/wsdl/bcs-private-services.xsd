<?xml version="1.0" encoding="UTF-8"?>
<schema
	targetNamespace="http://www.bcs.io/fileserver/api/types/internal"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.bcs.io/fileserver/api/types/internal"
	xmlns:global="http://www.bcs.io/global/types"
	xmlns:common="http://www.bcs.io/fileserver/types">
	<import schemaLocation="../xsd/global-types.xsd"
		namespace="http://www.bcs.io/global/types">
	</import>
	<import schemaLocation="../xsd/fileserver-types.xsd"
		namespace="http://www.bcs.io/fileserver/types"></import>

	<complexType name="GetDirectContentReadUrlRq_Type">
		<sequence>
			<element name="locator"
				type="common:ContentLocator_Type" minOccurs="1" maxOccurs="1">
			</element>
            <element name="fragment" type="common:ContentFragment_Type"
				minOccurs="1" maxOccurs="1">
			</element>
		</sequence>
	</complexType>

	<element name="GetDirectContentReadUrlRq"
		type="tns:GetDirectContentReadUrlRq_Type" />

	<complexType name="GetDirectContentReadUrlRs_Type">
		<complexContent>
			<extension base="global:ServiceResponse_Type">
				<sequence>
					<element name="contentRef" type="common:URL_Type"
						minOccurs="1" maxOccurs="1"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<element name="GetDirectContentReadUrlRs"
		type="tns:GetDirectContentReadUrlRs_Type" />






	<complexType name="WarmContentUpRq_Type">
		<sequence>
			<element name="fileName" type="common:FileName_Type"></element>
		</sequence>
	</complexType>
	
    <complexType name="WarmContentUpRs_Type">
    	<complexContent>
    		<extension base="global:ServiceResponse_Type"></extension>
    	</complexContent>
    </complexType>
    
    <complexType name="CoolContentDownRq_Type">
    	<sequence>
    		<element name="fileName" type="common:FileName_Type"></element>
    	</sequence>
    </complexType>
    
    <complexType name="CoolContentDownRs_Type">
    	<complexContent>
    		<extension base="global:ServiceResponse_Type"></extension>
    	</complexContent>
    </complexType>

    <element name="CoolContentDownRq" type="tns:CoolContentDownRq_Type"></element>

    <element name="CoolContentDownRs" type="tns:CoolContentDownRs_Type"></element>

    <element name="WarmContentUpRq" type="tns:WarmContentUpRq_Type"></element>

    <element name="WarmContentUpRs" type="tns:WarmContentUpRs_Type"></element>
</schema>