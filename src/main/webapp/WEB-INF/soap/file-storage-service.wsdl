<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://www.bcs.io/fileserver/soap/endpoints/files"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="WSFileService"
	targetNamespace="http://www.bcs.io/fileserver/soap/endpoints/files"
	xmlns:storage="http://www.bcs.io/fileserver/soap/types/file/storage"
	xmlns:global="http://www.bcs.io/fileserver/soap/types/common">

	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import
				namespace="http://www.bcs.io/fileserver/soap/types/file/storage"
				schemaLocation="file-storage-types.xsd">
			</xsd:import>
			<xsd:import
				namespace="http://www.bcs.io/fileserver/soap/types/common"
				schemaLocation="global-types.xsd">
			</xsd:import>
		</xsd:schema>
	</wsdl:types>


	<wsdl:message name="createFileRequest">
		<wsdl:part name="parameters" element="storage:CreateFileRq"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="createFileResponse">
		<wsdl:part name="parameters" element="storage:CreateFileRs"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="createFileFault">
		<wsdl:part name="parameters" element="global:ServiceFault"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="disposeFileRequest">
		<wsdl:part name="parameters" element="storage:DisposeFileRq"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="disposeFileResponse">
		<wsdl:part name="parameters" element="storage:DisposeFileRs"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="disposeFileFault">
		<wsdl:part name="parameters" element="global:ServiceFault"></wsdl:part>
	</wsdl:message>
	<wsdl:portType name="WSFileService">
		<wsdl:operation name="createFile">
			<wsdl:input message="tns:createFileRequest"></wsdl:input>
			<wsdl:output message="tns:createFileResponse"></wsdl:output>
			<wsdl:fault name="fault" message="tns:createFileFault"></wsdl:fault>
		</wsdl:operation>
		<wsdl:operation name="disposeFile">
			<wsdl:input message="tns:disposeFileRequest"></wsdl:input>
			<wsdl:output message="tns:disposeFileResponse"></wsdl:output>
			<wsdl:fault name="fault" message="tns:disposeFileFault"></wsdl:fault>
		</wsdl:operation>
	</wsdl:portType>	<wsdl:binding name="WSFileServiceBinding"
		type="tns:WSFileService">

		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="createFile">

			<soap:operation
				soapAction="http://www.bcs.io/fileserver/soap/endpoints/files/createFile" />
			<wsdl:input>

				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>

				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="fault">

				<soap:fault use="literal" name="fault" />
			</wsdl:fault>
		</wsdl:operation>
		<wsdl:operation name="disposeFile">

			<soap:operation
				soapAction="http://www.bcs.io/fileserver/soap/endpoints/files/disposeFile" />
			<wsdl:input>

				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>

				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="fault">

				<soap:fault use="literal" name="fault" />
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="WSFileServiceEndpoint">
		<wsdl:port binding="tns:WSFileServiceBinding"
			name="WSFileServicePort">
			<soap:address
				location="http://www.bincloud.io/bcs/WSFileService" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
